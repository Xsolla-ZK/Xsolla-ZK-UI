#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/knuckleswolf/work/www/xsolla-zk/Xsolla-ZK-UI/node_modules/.pnpm/react-native@0.77.0_@babel+core@7.26.0_@babel+preset-env@7.26.7_@babel+core@7.26.0__@types+react@18.3.18_react@19.0.0/node_modules/react-native/node_modules:/Users/knuckleswolf/work/www/xsolla-zk/Xsolla-ZK-UI/node_modules/.pnpm/react-native@0.77.0_@babel+core@7.26.0_@babel+preset-env@7.26.7_@babel+core@7.26.0__@types+react@18.3.18_react@19.0.0/node_modules:/Users/knuckleswolf/work/www/xsolla-zk/Xsolla-ZK-UI/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/knuckleswolf/work/www/xsolla-zk/Xsolla-ZK-UI/node_modules/.pnpm/react-native@0.77.0_@babel+core@7.26.0_@babel+preset-env@7.26.7_@babel+core@7.26.0__@types+react@18.3.18_react@19.0.0/node_modules/react-native/node_modules:/Users/knuckleswolf/work/www/xsolla-zk/Xsolla-ZK-UI/node_modules/.pnpm/react-native@0.77.0_@babel+core@7.26.0_@babel+preset-env@7.26.7_@babel+core@7.26.0__@types+react@18.3.18_react@19.0.0/node_modules:/Users/knuckleswolf/work/www/xsolla-zk/Xsolla-ZK-UI/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/react-native@0.77.0_@babel+core@7.26.0_@babel+preset-env@7.26.7_@babel+core@7.26.0__@types+react@18.3.18_react@19.0.0/node_modules/react-native/cli.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/react-native@0.77.0_@babel+core@7.26.0_@babel+preset-env@7.26.7_@babel+core@7.26.0__@types+react@18.3.18_react@19.0.0/node_modules/react-native/cli.js" "$@"
fi
